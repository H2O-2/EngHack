{
  "manifest_version": 2,

  "name": "EngHack",
  "description": "EngHack Game",
  "version": "1.0",

  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["bower_components/jQuery/dist/jquery.min.js", "js/global.js"]
    }
  ],
  "permissions": [
    "tabs", "<all_urls>"
  ]
}